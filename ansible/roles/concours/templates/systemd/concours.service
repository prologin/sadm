[Unit]
Description=Concours website
After=network-online.target postgresql.service
Requires=postgresql.service

[Service]
Type=simple
User=concours
WorkingDirectory=/opt/prologin/concours
ExecStart=/opt/prologin/venv/bin/gunicorn -w 4 -b 127.0.0.1:20150 prologin.concours.wsgi

[Install]
WantedBy=multi-user.target
